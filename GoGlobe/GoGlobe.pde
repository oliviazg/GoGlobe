    if (levels==level.length) { //once you've completed all the levels
      println("CONGRATS!"); //print congratulatory message
      println("You've completed all the levels!"); 
    }
    levels=levels%level.length; //
    win=0;
    player.prep();
  }
  if (countdown > 0) {
    countdown --;
  }
  text(countdown, 20, 20);
  int time = 59;
}

//movement of ball using arrow keys
void keyReleased() {
  if (key==' ') {
    s_s=0;
    p.sd=1;
  }
  if (keyCode==LEFT) {
    a_a=0;
    key_logAdd(2);
  }
  if (keyCode==RIGHT) {
    d_d=0;
    key_logAdd(4);
  }
  if (keyCode==UP) {
    w_w=0;
    key_logAdd(1);
  }
  if (keyCode==DOWN) {
    s_sp=0;
    key_logAdd(3);
  }
  if(key=='r' || key=='R'){
    key_logAdd(5);
  }
}
void keyPressed() {
  if (key==' ') {
    s_s=1;
  }
  if (keyCode==LEFT) {
    a_a=1;
  }
  if (keyCode==RIGHT) {
    d_d=1;
  }
  if (keyCode==UP) {
    w_w=1;
  }
  if (keyCode==DOWN) {
    s_sp=1;
  }
